<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<SplitPane dividerPositions="0.07534246575342465" minHeight="700.0" minWidth="1024.0" style="-fx-background-color: black;" stylesheets="@../css/custom.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
    <items>
        <VBox maxWidth="74.0" spacing="10.0" style="-fx-background-color: #575757;" SplitPane.resizableWithParent="false">
            <children>
                <ImageView fx:id="menuHome" fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../images/home.png" />
                    </image>
                    <VBox.margin>
                        <Insets left="5.0" right="5.0" top="10.0" />
                    </VBox.margin>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </ImageView>

                <ImageView fx:id="menuWebAdmin" fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../images/web-admin.png" />
                    </image>
                    <VBox.margin>
                        <Insets left="5.0" />
                    </VBox.margin>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </ImageView>

                <ImageView fx:id="menuMaps" fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../images/maps.png" />
                    </image>
                    <VBox.margin>
                        <Insets left="5.0" />
                    </VBox.margin>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </ImageView>

                <ImageView fx:id="menuInstall" fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../images/install.png" />
                    </image>
                    <VBox.margin>
                        <Insets left="5.0" />
                    </VBox.margin>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </ImageView>

                <ImageView fx:id="menuHelp" fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../images/help.png" />
                    </image>
                    <VBox.margin>
                        <Insets left="5.0" />
                    </VBox.margin>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </ImageView>
            </children>
        </VBox>

        <ScrollPane fx:id="content" />
     </items>
</SplitPane>
